<!-- LAYERS -->


<Layer name="clade" status="on" srs="&osm2pgsql_projection;">
  <StyleName>clade</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,clade,name,zoomview
      from &prefix;_polygon
    ) as buildings</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>

<Layer name="branches" status="on" srs="&osm2pgsql_projection;">
  <StyleName>branches</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,branch,zoomview
      from &prefix;_line
      where branch is not null          
      ) as roads
    </Parameter>
    &datasource-settings;
  </Datasource>
</Layer>

<Layer name="Clade-names" status="on" srs="&osm2pgsql_projection;">
  <StyleName>Clade_names</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,cladecenter,zoomview,name
      from &prefix;_point
      where cladecenter is not null
    ) as placenames</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>

<Layer name="nodal" status="on" srs="&osm2pgsql_projection;">
  <StyleName>nodal</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,nodal,tip,zoomview,name
      from &prefix;_point
      where cladecenter is null
    ) as points</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>


<Layer name="tip_names" status="on" srs="&osm2pgsql_projection;">
  <StyleName>tip_names</StyleName>
  <Datasource>
    <Parameter name="table">
      (select way,nodal,name,tip,zoomview
      from &prefix;_point where tip is not null
    ) as placenames</Parameter>
    &datasource-settings;
  </Datasource>
</Layer>


</Map>
